                                    <div class="controls">
                                        @Html.TextBoxFor(model => model.Productapplications, new { @class = "input-xlarge span6" })
                                        <p class="help-block"><i class="warning">@Html.ValidationMessageFor(model => model.Productapplications)</i></p>
                                    </div>
