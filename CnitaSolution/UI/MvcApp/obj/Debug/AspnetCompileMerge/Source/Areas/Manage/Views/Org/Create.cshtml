                                        <p class="help-block"><i class="warning">@Html.ValidationMessageFor(model => model.Addressen)</i></p>
                                    </div>
                                </div>

                                @*英文地址*@
                                <div class="control-group">
                                    <label class="control-label" for="input01">@Html.LabelFor(model => model.Addressen)</label>
                                    <div class="controls">
                                        @Html.TextBoxFor(model => model.Addressen, new { @class = "input-xlarge span6" })
                                        <p class="help-block"><i class="warning">@Html.ValidationMessageFor(model => model.Addressen)</i></p>
                                    </div>
                                </div>

