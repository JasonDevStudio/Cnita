                                    </div>
                                </div>

                                @*主要联系人手机*@
                                <div class="control-group">
                                    <label class="control-label" for="input01">@Html.LabelFor(model => model.Contactphone)</label>
                                    <div class="controls">
                                        @Html.TextBoxFor(model => model.Contactphone, new { @class = "input-xlarge span6" })
                                        <p class="help-block"><i class="warning">@Html.ValidationMessageFor(model => model.Contactphone)</i></p>
                                    </div>
                                </div>

                                @*主要联系人Email*@
                                <div class="control-group">
                                    <label class="control-label" for="input01">@Html.LabelFor(model => model.Contactemail)</label>
                                    <div class="controls">
                                        @Html.TextBoxFor(model => model.Contactemail, new { @class = "input-xlarge span6" })
                                        <p class="help-block"><i class="warning">@Html.ValidationMessageFor(model => model.Contactemail)</i></p>
                                    </div>
                                </div>

                                @*单位类别*@
                                <div class="control-group">
                                    <label class="control-label" for="input01">@Html.LabelFor(model => model.Unittype)</label>
                                    <div class="controls">
                                        
