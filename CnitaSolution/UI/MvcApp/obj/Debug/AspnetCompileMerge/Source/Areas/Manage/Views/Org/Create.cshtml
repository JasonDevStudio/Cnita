                                <div class="control-group">
                                    <label class="control-label" for="input01">@Html.LabelFor(model => model.Membershiplevel)</label>
                                    <div class="controls">
                                        @Html.TextBoxFor(model => model.Membershiplevel, new { @class = "input-xlarge span6" })
                                        <p class="help-block"><i class="warning">@Html.ValidationMessageFor(model => model.Membershiplevel)</i></p>
                                    </div>
                                </div>

                                @*单位简介*@
                                <div class="control-group">
                                    <label class="control-label" for="input01">@Html.LabelFor(model => model.Introduction)</label>
                                    <div class="controls">
                                        @Html.TextAreaFor(model => model.Introduction, new { @class = "input-xlarge span9" ,style="height:150px;" })
                                        <p class="help-block"><i class="warning">@Html.ValidationMessageFor(model => model.Introduction)</i></p>
                                    </div>
                                </div>

                                <div class="control-group">
                                    <!-- Button -->
                                    <div class="controls">
                                        <button class="btn red span2" type="submit" id="btnSubmit" onclick="Loading(this)">提交 <i class="m-icon-swapright m-icon-white"></i></button>

                                    </div>
                                </div>
