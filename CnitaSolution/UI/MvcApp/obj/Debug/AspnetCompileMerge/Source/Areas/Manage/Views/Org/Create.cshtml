                    </div>
                </div>
            </div>
        </div>

    </div>
</div>


@section Styles{
}
@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
    @*实例化 编辑器*@
    <script type="text/javascript">
        $(document).ready(function () {

        });
        function Goto() {
            window.location = "@Url.Action("Index", "Org")";
        }
        function Loading(obj) {
            //$(obj).hide();
        }
        function BtnShow() {
            $("#btnSubmit").show();
        }
    </script>
    @Html.Raw(ViewBag.CustomScript)
}







