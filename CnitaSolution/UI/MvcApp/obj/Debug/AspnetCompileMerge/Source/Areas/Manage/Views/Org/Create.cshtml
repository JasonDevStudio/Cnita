@model Library.Models.ModelOrganization

@{
    ViewBag.Title = "编辑/新增 会员信息";
    Layout = "~/Areas/Manage/Views/Shared/_manage.cshtml";
}


<div class="container-fluid">
    <!-- BEGIN PAGE HEADER-->
    <div class="row-fluid">
        <div class="span12">
            <!-- BEGIN PAGE TITLE & BREADCRUMB-->
            <h3 class="page-title">
                <!--Jason.Yao <small>后台管理系统</small>-->
            </h3>
            <ul class="breadcrumb">
                <li><i class="icon-home"></i><a href="index.html">后台管理</a>                <i class="icon-angle-right"></i></li>
                <li><a href="@Url.Action("Index", "Org")">会员管理</a><i class="icon-angle-right"></i></li>
                <li><a href="@Url.Action("Create", "Org")">编辑/新增 会员信息</a></li>
                <li class="pull-right no-text-shadow">
                    <div id="dashboard-report-range"
                        class="dashboard-date-range tooltips no-tooltip-on-touch-device responsive" data-tablet=""
                        data-desktop="tooltips" data-placement="top" data-original-title="Change dashboard date range">
                        <i class="icon-calendar"></i><span></span><i class="icon-angle-down"></i>
                    </div>
                </li>
            </ul>
            <!-- END PAGE TITLE & BREADCRUMB-->
        </div>
    </div>
    <!-- END PAGE HEADER-->
    <!-- BEGIN PAGE CONTAINER-->
    <div id="dashboard">
        <div class="row-fluid">
            <div class="span12">
                <div class="portlet box red">
                    <div class="portlet-title">
                        <div class="caption"><i class="icon-cogs"></i>新增/编辑 会员信息</div>
                        <div class="tools">
                            <a href="" class="collapse"></a><a href="" class="remove"></a>
                        </div>
                    </div>
                    <div class="portlet-body">
                        <div class="row-fluid">
                            <!--表单 开始-->
                            <form id="mainForm" action="@Url.Action("Create", "Org")" class="form-horizontal" enctype="multipart/form-data" method="post" >
                                @Html.AntiForgeryToken()
                                @Html.ValidationSummary(true)

                                @*名称*@
                                <div class="control-group">
                                    <label class="control-label" for="input01">@Html.LabelFor(model => model.Orgname)</label>
                                    <div class="controls">
                                        @Html.TextBoxFor(model => model.Orgname, new { @class = "input-xlarge span6" })
                                        <p class="help-block"><i class="warning">@Html.ValidationMessageFor(model => model.Orgname)</i></p>
                                    </div>
                                </div>

                                @*英文名称*@
                                <div class="control-group">
                                    <label class="control-label" for="input01">@Html.LabelFor(model => model.Orgenname)</label>
                                    <div class="controls">
                                        @Html.TextBoxFor(model => model.Orgenname, new { @class = "input-xlarge span6" })
                                        <p class="help-block"><i class="warning">@Html.ValidationMessageFor(model => model.Orgenname)</i></p>
                                    </div>
                                </div>

                                @*地址*@
                                <div class="control-group">
                                    <label class="control-label" for="input01">@Html.LabelFor(model => model.Address)</label>
