                                    <label class="control-label" for="input01">@Html.LabelFor(model => model.Processcategory)</label>
                                    <div class="controls">
                                        @Html.TextBoxFor(model => model.Processcategory, new { @class = "input-xlarge span6" })
                                        <p class="help-block"><i class="warning">@Html.ValidationMessageFor(model => model.Processcategory)</i></p>
                                    </div>
                                </div>

