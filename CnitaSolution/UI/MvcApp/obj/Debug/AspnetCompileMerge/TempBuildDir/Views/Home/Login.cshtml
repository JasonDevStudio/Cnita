	<script src="@Url.Content("~/Content/themes/assets/plugins/backstretch/jquery.backstretch.min.js")" type="text/javascript"></script>
	<script type="text/javascript" src="@Url.Content("~/Content/themes/assets/plugins/select2/select2.min.js")"></script>
	<!-- END PAGE LEVEL PLUGINS -->
	<!-- BEGIN PAGE LEVEL SCRIPTS -->
	<script src="@Url.Content("~/Content/themes/assets/scripts/app.js")" type="text/javascript"></script>
	<script src="@Url.Content("~/Content/themes/assets/scripts/login-soft.js")" type="text/javascript"></script>      
	<!-- END PAGE LEVEL SCRIPTS --> 
	<script>
	    jQuery(document).ready(function () {
	        App.init();
	        Login.init();
	    });
	</script>
